<h2 class="main-title">ویرایش پروفایل</h2>
<div class="card-body" id="profileSection">
    <div class="row">
        <div class="col-xs-12">
            <div ng-show="Model.errors.length" class="alert alert-danger">
                <ul ng-repeat="row in Model.errors">
                    <li>{{row}}</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label">نام</label>
        <input type="text" class="form-control" ng-model="Model.FirstName" />
    </div>
    <div class="form-group">
        <label class="control-label">نام خانوادگی</label>
        <input type="text" class="form-control" ng-model="Model.LastName" />
    </div>
    <div class="form-group">
        <label class="control-label">شماره تلفن</label>
        <input type="text" class="form-control" ng-model="Model.CellPhone" />
    </div>
    <div class="form-group">
        <label class="control-label">کد ملی</label>
        <input type="text" class="form-control" ng-model="Model.NationalCode" />
    </div>
</div>
<div class="btn-group-justified pull-left mb-5">
    <button type="button" class="btn btn-primary text-white" ng-click="save();">ثبت</button>
</div>

<toaster-container toaster-options="{'position-class': 'toast-top-center', 'newest-ontop': 'false'}"></toaster-container>